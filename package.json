{
   "name": "vitele-electron-forge",
   "productName": "vitele-electron-forge",
   "version": "1.0.0",
   "description": "Vitele Electron scaffolding using electron-forge",
   "main": "dist/index.js",
   "scripts": {
      "dev": "concurrently \"vite\" \"rimraf dist && tsc --project tsconfig.electron.json && cross-env NODE_ENV=development electron-forge start",
      "build": "rimraf out dist && vue-tsc --noEmit && vite build && tsc --project tsconfig.electron.json\"",
      "package": "yarn build && electron-forge package",
      "make": "yarn build && electron-forge make",
      "publish": "electron-forge publish",
      "lint": "eslint --ext .js,.ts,.vue ."
   },
   "keywords": [],
   "author": {
      "name": "DaPotatoMan",
      "email": "dev.iamfahad@gmail.com"
   },
   "license": "MIT",
   "config": {
      "forge": "./config/forge.config.js"
   },
   "dependencies": {
      "electron-squirrel-startup": "^1.0.0",
      "vue": "^3.2.19",
      "vue-router": "^4.0.11"
   },
   "devDependencies": {
      "@electron-forge/cli": "^6.0.0-beta.61",
      "@electron-forge/maker-deb": "^6.0.0-beta.61",
      "@electron-forge/maker-rpm": "^6.0.0-beta.61",
      "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
      "@electron-forge/maker-zip": "^6.0.0-beta.61",
      "@typescript-eslint/eslint-plugin": "^4.32.0",
      "@typescript-eslint/parser": "^4.32.0",
      "@vitejs/plugin-vue": "^1.9.2",
      "@vue/compiler-sfc": "^3.2.19",
      "concurrently": "^6.2.2",
      "cross-env": "^7.0.3",
      "electron": "15.0.0",
      "eslint": "^7.6.0",
      "eslint-config-airbnb-typescript": "^12.3.1",
      "eslint-import-resolver-typescript": "^2.5.0",
      "eslint-plugin-import": "^2.24.2",
      "eslint-plugin-vue": "^7.18.0",
      "typescript": "^4.4.3",
      "unplugin-auto-import": "^0.4.8",
      "unplugin-vue-components": "^0.15.4",
      "vite": "^2.6.0",
      "vite-plugin-pages": "^0.18.1",
      "vite-plugin-vue-layouts": "^0.5.0",
      "vite-plugin-windicss": "^1.4.8",
      "vite-tsconfig-paths": "^3.3.14",
      "vue-tsc": "^0.2.3",
      "windicss": "^3.1.8"
   }
}
